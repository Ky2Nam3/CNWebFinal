
@{
    Layout = null;
}

<!DOCTYPE html>

@using CoffeeDatDat.Models
@model  KhachHang
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>LoadChiTietKhachHang</title>
</head>
<body>
    <div class="content">

        <div class="module">
            <div class="module-head">
                <h3>Tables</h3>
            </div>
            <div class="module-body">
                <table class="table table-striped table-bordered table-condensed">
                    <thead>
                        <tr>
                            <th>Mã khách hàng</th>
                            <th>Họ Tên</th>
                            <th>SDT</th>
                            <th>Email</th>
                            <th>Tên Đăng Nhập</th>
                            <th>Mật Khẩu</th>
                        </tr>
                    </thead>
                    <tbody>
                            <tr>
                                <td>@Model.MaKH</td>
                                <td>@Model.HoTenKH</td>
                                <td>@Model.SDT</td>
                                <td>@Model.Email</td>
                                <td>@Model.TenDangNhap</td>
                                <td>@Model.MatKhau</td>
                            </tr>
                    </tbody>
                </table>
            </div>

            <div class="module-head">
                <h3></h3>
            </div>
        </div>



        <br />

    </div><!--/.content-->
</body>
</html>
